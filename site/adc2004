<script type='text/javascript'>
	var revSciArry = new Array();
	var rsinetsegsLngth =  rsinetsegs.length;
	var revSciCount = 0;
	var revSciChar = 0;

	if( rsinetsegsLngth > 10 )
	{
		revSciCount = 10;
	}
	else
	{
		revSciCount = rsinetsegsLngth;
	}
	
	for ( g = 0; g < 10; g++ )
	{
		adc6Array[g+7] = '';
	}
	
	for ( h = 0; h < revSciCount; h++ )
	{	
		revSciChar = rsinetsegs[h].indexOf('_');
		if( revSciChar >= 0 )
		{
			revSciArry[h] = rsinetsegs[h].substring( revSciChar + 1 );
			adc6Array[h+7] = revSciArry[h];
		}
	}
	createAdCookie(adc6Array);

  
	
	
</script>
