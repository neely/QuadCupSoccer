            <script>
                OAS_AD(adPosTop);
                function adplacement_120()
                {
                    document.getElementById('accuAdDiv').style.top = '65px';
                    if (document.getElementById)
                    { // DOM3 = IE5, NS6
                        document.getElementById('accuDiv').style.visibility = 'visible';
                    }
                    else
                    {
                        if (document.layers)
                        { // Netscape 4
                            document.hideShow.visibility = 'accuDiv';
                        }
                        else
                        { // IE 4
                            document.all.hideShow.style.visibility = 'accuDiv';
                        }
                    }
                }                      
            </script>

