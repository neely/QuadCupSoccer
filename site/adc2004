	<script type="text/javascript">
		function bugfixFFoxMacDivScroll(overflowPropValue)
		{
			if((browser.isMac)&&(browser.isFirefox))
			{
				var orBoxHead = document.getElementById('content_box_headlines');
				var orBoxBlog = document.getElementById('content_box_blogs');
				orBoxHead.style.overflow = overflowPropValue;
				orBoxBlog.style.overflow = overflowPropValue;			
			}		
		}
	</script>
