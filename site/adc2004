	<script type="text/javascript">
		var ranNum = Math.floor(Math.random()*10);
		var active = new Array(10);
		var whosActive;

		active[0] = 'headline';
		active[1] = 'blog';
		active[2] = 'headline';
		active[3] = 'headline';
		active[4] = 'headline';
		active[5] = 'headline';
		active[6] = 'headline';
		active[7] = 'headline';
		active[8] = 'blog';
		active[9] = 'headline';
		
		
		// we want to check if we are on headline page, if so, we want to display headlines
        var strScriptName = "/index.asp";
                
        if( strScriptName == "/news-story.asp" || strScriptName == "/news-top-headline.asp" || strScriptName == "/news-seasonal-headlines.asp" )
            whosActive = 'headline';
        else if (strScriptName =="/news-blogs.asp")
			// if a blog page, show blogs
			whosActive = 'blog';
        else    
		    // else let it be random
        	whosActive = active[ranNum];
		if (whosActive == 'headline')
		{
			changeActiveState('headline_box');
			changeInActiveState('blog_box');
			changeUsNewsView('content_box_blogs','content_box_headlines');
		}
		else
		{
			changeActiveState('blog_box');
			changeInActiveState('headline_box');
			changeUsNewsView('content_box_headlines','content_box_blogs')
		}				
	</script>
  
