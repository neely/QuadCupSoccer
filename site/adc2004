<script type="text/javascript">
var imgLevel;
var isRadAnim

isRadAnim = 0;
imgLevel = 'rad';
whyLevel = 'Summary';
var whyCount;
whyCount = 0;
function switchWhyTimer()
{
	alert("whyTimer is in use -- Find me");
	switch(whyCount)
	{
		case 0:
			whyCount=1;
			switchWhy('Summary');
			break;
		case 1:
			whyCount=2;
			switchWhy('History');
			break;
		case 2:
			whyCount=3;
			switchWhy('Innovation');
			break;
		case 3:
			whyCount=4;
			switchWhy('Accuracy');
			break;
		case 4:
			whyCount=0;
			switchWhy('Expertise');
			break;
	}
}


function switchWhy(whyType)
{
	var oldLink;
	var newLink;
	var navArrow = document.getElementById('nav_arrow_why');
	if(navArrow.style){styleRef = ".style";}

	oldLink = 'why' + whyLevel;
	
	switch(whyType)
	{
		case 'Summary':
			showHideContent(oldLink, false);
			showHideContent("whySummary", true);
			eval("navArrow" + styleRef + ".left = '37px';"); 
			document["whyPic"].src = "http://vortex.accuweather.com/adc2004/pub/images/2006/why_accuweather_01.jpg";
			break;

		case 'History':
			showHideContent(oldLink, false);
			showHideContent("whyHistory", true);
			eval("navArrow" + styleRef + ".left = '124px';"); 
			document["whyPic"].src = "http://vortex.accuweather.com/adc2004/pub/images/2006/why_accuweather_02.jpg";
			break;

		case 'Innovation':
			showHideContent(oldLink, false);
			showHideContent("whyInnovation", true);
			eval("navArrow" + styleRef + ".left = '211px';"); 
			document["whyPic"].src = "http://vortex.accuweather.com/adc2004/pub/images/2006/why_accuweather_03.jpg";
			break;

		case 'Accuracy':
			showHideContent(oldLink, false);
			showHideContent("whyAccuracy", true);
			eval("navArrow" + styleRef + ".left = '298px';"); 
			document["whyPic"].src = "http://vortex.accuweather.com/adc2004/pub/images/2006/why_accuweather_04.jpg";
			break;

		case 'Expertise':
			showHideContent(oldLink, false);
			showHideContent("whyExpertise", true);
			eval("navArrow" + styleRef + ".left = '385px';"); 
			document["whyPic"].src = "http://vortex.accuweather.com/adc2004/pub/images/2006/why_accuweather_05.jpg";
			break;
	}
	whyLevel = whyType;
	whyHighlight();
}

function switchRadar()
{
	if(isRadAnim == 0)
	{
		isRadAnim = 1;
	}
	else
	{
		isRadAnim = 0;
	}
	switchImage('rad');
}
function whyHighlight()
{
	var whyNav;
	whyNav = document.getElementById('whySummaryNav');
	if(whyLevel == 'Summary')
		changeClass(whyNav,'content_box_rednavbutton');			
	else
		changeClass(whyNav,'content_box_bluenavbutton');			
	
	whyNav = document.getElementById('whyHistoryNav');
	if(whyLevel == 'History')
		changeClass(whyNav,'content_box_rednavbutton');			
	else
		changeClass(whyNav,'content_box_bluenavbutton');			
	
	whyNav = document.getElementById('whyInnovationNav');
	if(whyLevel == 'Innovation')
		changeClass(whyNav,'content_box_rednavbutton');			
	else
		changeClass(whyNav,'content_box_bluenavbutton');			
	
	whyNav = document.getElementById('whyAccuracyNav');
	if(whyLevel == 'Accuracy')
		changeClass(whyNav,'content_box_rednavbutton');			
	else
		changeClass(whyNav,'content_box_bluenavbutton');			
	
	whyNav = document.getElementById('whyExpertiseNav');
	if(whyLevel == 'Expertise')
		changeClass(whyNav,'content_box_rednavbutton');			
	else
		changeClass(whyNav,'content_box_bluenavbutton');			
	
} 
function switchImage(imgType)
{
	var styleRef = "";
	var navArrow = document.getElementById('nav_arrow_natoverview');
	if(navArrow.style){styleRef = ".style";}
	var oldLink;
	var newLink;
	var advImg = new Image;
	var advHeight = 0;
	advImg.src = "http://sirocco.accuweather.com/adc_images2/english/current/svrwx/swskeys.gif";
	var advHeight = advImg.height;
	if (advHeight > 76) advHeight = 76;
	oldLink = imgLevel + "Link";

	//alert( "Turning off: " + oldLink );

	switch(imgType)
	{
		case 'rad':
		
			if(isRadAnim == 1)
			{
				document["map_image"].src = "http://sirocco.accuweather.com/nx_mosaic_400x300_public/SIR/inmaSIRUS_.gif";
			}
			else
			{
				document["map_image"].src = "http://sirocco.accuweather.com/nx_mosaic_400x300_public/SIR/inmSIRUS_.gif";
			}
			document["map_image"].height = 334;
			document["map_image"].useMap = '#smallmap';
			document["map_key"].src = "http://vortex.accuweather.com/adc2004/common/images/keys/400x40/radar_sir_v3.gif"
			document["map_key"].height = 40;
			document.getElementById( "clickableText" ).style.visibility = "visible";
			showHideContent(oldLink, false);
			showHideContent("radLink", true);
			eval("navArrow" + styleRef + ".left = '25px';"); 
			break;
		case 'sat':
			document["map_image"].src = "http://sirocco.accuweather.com/sat_mosaic_400x300_public/EI/iseun.jpg";
			document["map_image"].height = 334;
			document["map_image"].useMap = '#smallmap';
			document["map_key"].src = "http://vortex.accuweather.com/adc2004/common/images/keys/400x40/sat_ei.gif"
			document["map_key"].height = 40;
			document.getElementById("clickableText").style.visibility = "visible";
			showHideContent(oldLink, false);
			showHideContent("satLink", true);
			eval("navArrow" + styleRef + ".left = '83px';");
			break;
		case 'svr':
			document["map_image"].src = "http://sirocco.accuweather.com/adc_images2/english/forecast/svrwx/400x300/US__TODsvrwxrisk.gif";
			document["map_key"].src = "http://vortex.accuweather.com/adc2004/common/images/keys/400x40/severe_wx_risk.gif";
			document["map_key"].height = 40;
			document.getElementById( "clickableText" ).style.visibility = "visible";
			showHideContent(oldLink, false);
			showHideContent("svrLink", true);
			eval("navArrow" + styleRef + ".left = '167px';");
			break;
		case 'tropics':
			document["map_image"].src = "http://sirocco.accuweather.com/iwxpage/adc/popup/iws_tropical_4x3.jpg";
			document["map_image"].height = 334;
			document["map_image"].useMap = '#trop4x3';
			document["map_key"].src = "http://vortex.accuweather.com/adc2004/pub/images/misc/blank.gif";
			document["map_key"].height = 40;
			showHideContent(oldLink, false);
			showHideContent("tropicsLink", true);
			eval("navArrow" + styleRef + ".left = '399px';");
			break;
		case 'surface':
			document["map_image"].src = "http://sirocco.accuweather.com/adc_images2/english/forecast/sfc/400x300/us__todsfc.gif";
			document["map_image"].height = 318;
			document["map_key"].src = "http://vortex.accuweather.com/adc2004/common/images/keys/400x40/surface_fronts.gif";
			document["map_key"].height = 40;
		    document.getElementById( "clickableText" ).style.visibility = "hidden";
			showHideContent(oldLink, false);
			//showHideContent("surfLink", true);
			showHideContent("surfaceLink", true);
			eval("navArrow" + styleRef + ".left = '250px';");			
			break;
		case 'temp':
			//document["map_image"].src = "http://sirocco.accuweather.com/adc_images2/english/forecast/fhgh/400x300/US__TODhigh.gif";
			document["map_image"].src = "http://vortex.accuweather.com/publicgfx/usustodhighs_4x3_adc.jpg";
			document["map_image"].height = 334;
			document["map_image"].useMap = '#tempMap';
			document["map_key"].src = "http://vortex.accuweather.com/adc2004/common/images/keys/400x40/hires_temps.gif";
			document["map_key"].height = 40;
			//document["map_key"].src = "http://vortex.accuweather.com/adc2004/pub/images/misc/blank.gif";
			//document["map_key"].height = 40;
			document.getElementById( "clickableText" ).style.visibility = "visible";
			showHideContent(oldLink, false);
			showHideContent("tempLink", true);
			eval("navArrow" + styleRef + ".left = '322px';");
			break;
		case 'winter':
			document["map_image"].src = "http://sirocco.accuweather.com/adc_images2/english/forecast/snow/400x300/US__0024snow.gif";
			document["map_image"].height = 334;
			document["map_image"].useMap = '#smallmap';
			document["map_key"].src = "http://vortex.accuweather.com/adc2004/common/images/keys/400x40/precip_snowfall.gif";
			document["map_key"].height = 40;
			document.getElementById( "clickableText" ).style.visibility = "visible";
			showHideContent(oldLink, false);
			showHideContent("winterLink", true);
			eval("navArrow" + styleRef + ".left = '399px';");
			break;
		case 'adv':
			document["map_image"].src = "http://sirocco.accuweather.com/adc_images2/english/current/svrwx/400x300/isvrwxus_.gif";
			document["map_image"].height = 334;
			document["map_image"].useMap = '#severeMap';
			document["map_key"].src = "http://sirocco.accuweather.com/adc_images2/english/current/svrwx/swskeys.gif";
			document["map_key"].height = advHeight;
			document.getElementById("clickableText").style.visibility = "visible";
			showHideContent(oldLink, false);
			showHideContent("advLink", true);
			eval("navArrow" + styleRef + ".left = '167px';");
			break;
	}
	imgLevel = imgType;
	imageHighlight();
}

function showHideContent(id, show)
{
	var elem = document.getElementById(id);
	if (elem) 
	{
		if (show) 
		{
			elem.style.display = 'block';
			elem.style.visibility = 'visible';
		} 
		else
		{
			elem.style.display = 'none';
			elem.style.visibility = 'hidden';
		}
	}
}        
function clickReg(reg)
{
    var myreg = new String(reg);
    switch(imgLevel)
    {
		case 'rad':
		    document.location.href='http://www.accuweather.com/radar-regional.asp?partner=accuweather&traveler=0&level=regional&type=SIR&anim=STILL&site='+myreg;
		    break;
		case 'sat':
			document.location.href='http://www.accuweather.com/maps-satellite.asp?partner=accuweather&traveler=0&site=' + myreg.toUpperCase() + '&type=ei';
			break;
		case 'svr':
			document.location.href='http://www.accuweather.com/index-severe-weather.asp?partner=accuweather&traveler=0';
			break;
		case 'surface':
			document.location.href='http://www.accuweather.com/maps-surface.asp?partner=accuweather&traveler=0';
			break;
		case 'temp':
			document.location.href='http://www.accuweather.com/maps-temperature.asp?partner=accuweather&traveler=0&type=fhgh&site=us' + reg + '&fday=1&large=0';
			break;
		case 'winter':
			document.location.href='http://www.accuweather.com/maps-precipitation.asp?partner=accuweather&traveler=0&type=snow';
			break;
		case 'tropics':
			document.location.href = 'http://hurricane.accuweather.com/index.asp?partner=accuweather&traveler=0';
			break;
        case 'adv':
            document.location.href='http://www.accuweather.com/maps-watches.asp?partner=accuweather&traveler=0';
            break;			
    }
}

function clickState( state )
{
     var mystate = new String(state);
     
     switch( imgLevel )
     {
        case 'adv':
            document.location.href='http://www.accuweather.com/maps-watches.asp?partner=accuweather&traveler=0&traveler=0&type=WW&level=' + mystate.toUpperCase();
            break;
    }    
}
      
function myMouseOut(mObj, mStyle)
{
	changeClass(mObj, mStyle);
	imageHighlight();
}
function imageHighlight()
{
	var imageNav;	
	imageNav = document.getElementById('blue_navbar_radar');
	if(imgLevel == 'rad')
		changeClass(imageNav,'content_box_rednavbutton');			
	else
		changeClass(imageNav,'content_box_bluenavbutton');			

	imageNav = document.getElementById('blue_navbar_satellite');
	if(imgLevel == 'sat')
		changeClass(imageNav,'content_box_rednavbutton');			
	else
		changeClass(imageNav,'content_box_bluenavbutton');			

	imageNav = document.getElementById('blue_navbar_severe');
	if(imgLevel == 'svr' || imgLevel == 'adv' )
		changeClass(imageNav,'content_box_rednavbutton');			
	else
		changeClass(imageNav,'content_box_bluenavbutton');			

	imageNav = document.getElementById('blue_navbar_travel');
	if(imgLevel == 'surface')
		changeClass(imageNav,'content_box_rednavbutton');			
	else
		changeClass(imageNav,'content_box_bluenavbutton');			

	imageNav = document.getElementById('blue_navbar_temperature');
	if(imgLevel == 'temp')
		changeClass(imageNav,'content_box_rednavbutton');			
	else
		changeClass(imageNav,'content_box_bluenavbutton');			

	imageNav = document.getElementById('blue_navbar_pollen');
	if (imgLevel == 'tropics' || imgLevel == 'winter')
		changeClass(imageNav,'content_box_rednavbutton');			
	else
		changeClass(imageNav,'content_box_bluenavbutton');

}

</script>

