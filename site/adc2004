<script type="text/javascript" language="javascript" >
	
    /**
        Timer function that is used by entrySiteAd code to
        hide the entrySiteAd and show the ADC page
    */
    function UpdateTimer()
    {
        var entrySiteAd = document.getElementById( "entrySiteAd" );
        entrySiteAd.style.visibility = 'hidden';
        entrySiteAd.style.display = 'none';
        
        var page_container = document.getElementById( "page_container" );
        page_container.style.visibility = 'visible';
        page_container.style.display = 'block';
    } 
        
    var pageInfo = document.getElementById( "page_container" );
    var entrySiteAd = document.getElementById( "entrySiteAd" );

    
    // if this variable is set to 1, from dave kennedy's ad code, then we want to display
    // the entrySiteAd div layer over top of the page    
    // NOTE: The page is loaded in in the background 
    if( showhomedepot > 0 )
    {
        if( pageInfo.style )
        {
            pageInfo.style.visibility = 'hidden';
            pageInfo.style.display = 'none';
            
            entrySiteAd.style.visibility = 'visible';
            entrySiteAd.style.display = 'block';
        }
        else
        {
            pageInfo.visibility = 'hidden';
            pageInfo.display = 'none';                

            entrySiteAd.visibility = 'visible';
            entrySiteAd.display = 'block';
        }
        
        // Set a timer to go off in 15 seconds to hide the entrySiteAd
        //var timerID  = setTimeout("UpdateTimer()", 15000);

    }
            
</script>

  
		
